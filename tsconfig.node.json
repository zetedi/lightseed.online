{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client", "@types/node"]
  },
  "include": ["vite.config.ts"]
}